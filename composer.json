{
  "name": "justin-robinson/scoop",
  "description": "Scoop framework and orm for php 7+ and apache 2.4+",
  "minimum-stability": "stable",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Justin Robinson",
      "email": "jrob09bhs@gmail.com"
    }
  ],
  "keywords" : [
    "php", "php 7", "orm", "database", "mysql", "apache"
  ],
  "require": {
    "php": ">=5.6.0",
    "kevinlebrun/colors.php": "0.4.1",
    "filp/whoops": "2.*",
    "justin-robinson/lrucache": "*",
    "satooshi/php-coveralls": "^1.0"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "*",
    "phpunit/phpunit-mock-objects": "*",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-0": {
      "scoop\\": "scoop/"
    },
    "files": [
      "bootstrap.php"
    ]
  },
  "bin" : [
    "bin/scoop"
  ]
}
